{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": true,
		"removeComments": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"target": "es2017",
		"sourceMap": true,
		"outDir": "./dist",
		"baseUrl": "./",
		"incremental": true,
		"skipLibCheck": true,
		"strictNullChecks": false,
		"noImplicitAny": false,
		"strictBindCallApply": false,
		"forceConsistentCasingInFileNames": false,
		"noFallthroughCasesInSwitch": false,
		"paths": { "@/*": ["./src/*"] },
		"typeRoots": ["./node_modules/@types"],
		"types": ["node", "jest"]
	},
	// "validation":	{
	// 	"notExported": false
	// }, 
	"typedocOptions": {
		"entryPoints" : [ "src/common/helper/env.helper.ts",
					"src/shared/typeorm/typeorm.service.ts",
					"src/app.module.ts", "src/main.ts",
					"src/api/user/user.entity.ts",
					"src/api/user/auth/auth.helper.ts",
					"src/api/user/auth/auth.guard.ts",
					"src/api/user/auth/auth.strategy.ts",
					"src/api/user/auth/auth.service.ts",
					"src/api/user/auth/auth.dto.ts",
					"src/api/user/auth/auth.controller.ts",
					"src/api/user/auth/auth.module.ts" ],
		"out": "docs"
	}
}
